theme_bw()+
labs(x = "Time (min)", y = "Temperature (°C)")+
scale_color_discrete(name = "Temperature Profile")+
facet_wrap(~Condition, ncol= 1)+
theme(legend.position = "right")+
geom_text(data = dat_text, mapping = aes(x = x, y = y, label = label), size =3.5)
